
<link rel="stylesheet" href="/opendias/style/jquery-ui.css" type="text/css" />
<link rel="stylesheet" href="/opendias/style/jquery.tagsinput.css" type="text/css" />

<script type="text/javascript" src="/opendias/includes/sprintf-0.7-beta1.js"></script>
<script type="text/javascript" src="/opendias/includes/jquery-ui.min.js"></script>
<script type="text/javascript" src="/opendias/includes/jquery.tagsinput.js"></script>
<script type="text/javascript" src="/opendias/includes/local/generic.resource"></script>
<script type="text/javascript" src="/opendias/includes/local/openDias.docList.js.resource"></script>
<script type="text/javascript" src="/opendias/includes/openDias.docList.js"></script>
<script type="text/javascript" src="/opendias/includes/local/openDias.filter.js.resource"></script>
<script type="text/javascript" src="/opendias/includes/openDias.filter.js"></script>

<div id='filter'>
  <div id='filterOptions'>
    <div id='filterInner'>
      <h3>---LOCAL_doclist_filterby---</h3>
      <div class='row'>
        <div class='label'>
          ---LOCAL_doclist_title_or_ocr---
        </div>
        <div class='data'>
          <input id='textSearch' />
        </div>
      </div>
      <div class='row'>
        <div class='label'>
          ---LOCAL_doclist_has_action---
        </div>
        <div class='data'>
            <input type='checkbox' id='isActionRequired' />
        </div>
      </div>
      <div class='row'>
        <div class='label'>
          ---LOCAL_doclist_doc_date---
        </div>
        <div class='data'>
            <input id='startDate' /> - <input id='endDate' />
        </div>
      </div>
      <div class='row'>
        <div class='label'>
          ---LOCAL_doclist_doc_tag---
        </div>
        <div class='data'>
          <input id='tags' type='text' class='tags'>
        </div>
      </div>
      <div class='row'>
        <div class='label'>
          &nbsp;
        </div>
        <div class='data'>
          <div class='innerLeftDisplay'>
            <button id='doFilter'>---LOCAL_doclist_filterbutton---</button>
          </div>
          <div class='innerRightDisplay'>
            <div id="filterProgress"></div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div id='filterTab'>
    ---LOCAL_doclist_filtertab---
  </div>
</div>

<div id='nodocs'>
  ---LOCAL_doclist_nodocs---
</div>

<div id='doclist' style='display: none'>

  <h2>---LOCAL_doclist_title---</h2>

  <div class='tableheader'>
    <div id='sortbydocid' class='tabledocid'><span>---LOCAL_doclist_table_docid---</span><span class="ui-icon"></span></div>
    <div id='sortbytitle' class='tabletitle'><span>---LOCAL_doclist_table_title---</span><span class="ui-icon"></span></div>
    <div id='sortbytype' class='tabletype'><span>---LOCAL_doclist_table_type---</span><span class="ui-icon"></span></div>
    <div id='sortbydate' class='tabledate'><span>---LOCAL_doclist_table_date---</span><span class="ui-icon"></span></div>
  </div>

  <div id='results'>
    <div id='binding_block'>
    </div>
    <div id='loading'>---LOCAL_doclist_loading---&nbsp;.&nbsp;.&nbsp;.</div>
  </div>

</div>


